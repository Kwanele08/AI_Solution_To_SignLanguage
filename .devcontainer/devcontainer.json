{
    "name": "Python Dev Container",
    "image": "mcr.microsoft.com/devcontainers/pyhton:3.11-bullseye", 
    "extensions": [
        "ms.python.python",
        "ms.toolsai.jupyter"
    ],
    "settings": {
        "terminal.intergrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "pip install -r requirements.txt"
}